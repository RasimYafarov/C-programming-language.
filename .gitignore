import turtle
t = turtle.Turtle()
t.shape('turtle')
t.pensize(6)
t.pencolor('orange')
t.penup()
t.goto(0, -50)
t.pendown()
for i in range(4) :
  t.fd(85)
  t.rt(90)
t.pencolor('green')
t.penup()
t.goto(0, 50)
t.pendown()
for i in range(5):
